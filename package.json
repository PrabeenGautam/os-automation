    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  },
  "description": "CLI tool for automating OS workflows",
  "keywords": [
