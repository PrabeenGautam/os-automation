{
  "name": "os-automation",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "bin": {
    "osm": "dist/index.js"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
